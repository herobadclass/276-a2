<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <!-- <% include ../partials/header.ejs %> -->
  </head>
  <style media="screen">
    .A {
      border: 1px solid rgb(80, 140, 90);
      background-color: rgb(90, 160, 110);
    }
    .EE {
      border: 1px solid rgb(80, 100, 120);
      background-color: rgb(100, 120, 150);
    }
  </style>
  <body>
    <!-- <% include ../partials/nav.ejs %> -->
    <button type="button" name="adduser" onclick = "location.href='/adduser.html'">ADD USER</button>
    <div class="container" >
      <h2>Database Results</h2>
      <% rows.forEach(function(r) { %>
        <div class = '<%= r.type %>'
        style = 'width: <%= r.width %>px; height: <%= r.height %>px; position: relative; margin: 5px; float: left; text-align: center; line-height: <%= r.height %>px; font-family: Arial'
        onclick = "location.href='/user/<%= r.id %>'">
          <%= r.name %>
        </div>
      <% }); %>
    </div>
  </body>
</html>
